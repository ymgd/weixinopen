.list {
    height: 100%;
    display: flex;
    flex-direction: column;
    padding: 20rpx;
}

navigator {
    overflow: hidden;
}

.imgs {
    float: right;
}

.imgs image {
    display: block;
    width: 200rpx;
    height: 200rpx;
}

.infos {
    float: left;
    width: 480rpx;
    height: 200rpx;
    padding: 20rpx 0 0 20rpx;
}

.title {
    font-size: 20px;
}

.date {
    padding-top: 50rpx;
    float: right;
    font-size: 16px;
    color: #aaa;
    position: relative;
}

.classification {
    padding-top: 50rpx;
    font-size: 16px;
    color: #aaa;
    position: relative;
}
